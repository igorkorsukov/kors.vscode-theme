			"fontStyle": "italic"
		},
		"parameter": {
			"foreground": "#ffa348"
		},
		"property": {
			"foreground": "#ff6aad"
		},
		"property.readonly": {
			"foreground": "#ff6aad"
		},
		"function.global": {
			"foreground": "#74bbfb"
		},
		"function.method": {
			"foreground": "#74bbfb"
		},
		"namespace": "#cf6eff",
		"type": {
			"foreground": "#cf6eff"
		},
		"type.defaultLibrary": {
			"foreground": "#cf6eff"
		},
		"enumMember": {
			"foreground": "#cf6eff"
		},
		"modifiers": {
			"readonly": "#45c6d6",
			"static": "#45c6d6",
			"abstract": "#45c6d6",
			"declaration": "#45c6d6"
		}
