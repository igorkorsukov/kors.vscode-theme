	"semanticHighlighting": true,
	"semanticTokenColors": {
		"macro": "$palette.macro",
		"variable.fileScope": "$palette.var_global",
		"variable.classScope": "$palette.var_global",
		"variable.declaration": "$palette.var_global",
		"variable.readonly": "$palette.var_global",
		"variable.functionScope": "$palette.var_local",
		"variable.local": "$palette.var_local",
		"variable.defaultLibrary": "$palette.var_global",
		"namespace": "$palette.type",
		"class": "$palette.type",
		"type": "$palette.type",
		"enum": "$palette.type",
		"enumMember": "$palette.type",
		"typeParameter": "$palette.type",
		"property": "$palette.property",
		"function": "$palette.function",
		"method": "$palette.function",
		"parameter": "$palette.parameter",
		"keyword": "$palette.keyword",
		"operator": "$palette.operator"
